<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.BoardmanageDao">

    <select id="findBySearch" resultType="com.example.entity.Boardmanage">
        select `boardmanage`.* from `boardmanage`

        where 1 = 1
		<if test="search != null and search.boardcode != null and search.boardcode != ''">
			and `boardmanage`.`boardcode` like concat ('%', #{search.boardcode}, '%')
		</if>

    </select>

</mapper>